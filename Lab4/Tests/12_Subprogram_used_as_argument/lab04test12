program AnaliseDeTexto {{{global {int a;}logic function EhBooleano()statements{return true;}int function FuncaoZero(logic bool)statements {return 0;}mainstatements {/* ERRO SEMÃ‚NTICO ESPERADO: SUBPROGRAMA SENDO UTILIZADO COMO ARGUMENTO DE OUTRO SUBPROGRAMA */a = FuncaoZero(EhBooleano());}}}}