program AnaliseDeTexto {{{global {    int a;}/* ERRO SEMÂNTICO ESPERADO: VARIÁVEL DUPLAMENTE DECLARADA NO ESCOPO DE UM MÓDULO */procedure FuncaoTeste()local {    int i, i;}statements {    for i init 1 while i <= 5 new i + 1 do {        write ("O");    }}mainstatements {call FuncaoTeste();}}}}