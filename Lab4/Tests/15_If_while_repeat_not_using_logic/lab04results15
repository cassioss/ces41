program AnaliseDeTexto {{{

global {
	logic a;
}

procedure FuncaoTeste()
local {
	int i;
}

statements {
	if 1 then i = 0;
	while 1 do {
		i = i + 1;
	}
	repeat {
		write ("O\n");
	} while 0;
}

main
statements {
	call FuncaoTeste();
}

}}}


  =========================
  = TABELA  DE  SIMBOLOS: =
  =========================

Classe 5:
  (a, IDVAR, LOGICO, 0, 0, ##global)
  (##global, IDGLOB, NAOVAR, 0, 0)

Classe 8:
  (##main, IDPROG, NAOVAR, 0, 0, ##global)

Classe 13:
  (i, IDVAR, INTEIRO, 1, 1, FuncaoTeste)

Classe 17:
  (FuncaoTeste, IDPROC, NAOVAR, 0, 0, ##global)
