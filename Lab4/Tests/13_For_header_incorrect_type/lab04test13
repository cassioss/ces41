program AnaliseDeTexto {{{global {	logic a;}/* ERRO SEMÂNTICO ESPERADO: FOR RECEBE VARIÁVEL NÃO INTEIRA NO CABEÇALHO */procedure FuncaoTeste ()local {	int i;}statements {	
	for a init 1 while i <= 5 new i+1 do {
		write ("O");
	}
	
}
/*  Modulo principal  */mainstatements {	call FuncaoTeste ();}}}}