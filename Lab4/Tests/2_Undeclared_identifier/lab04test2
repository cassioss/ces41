program AnaliseDeTexto {{{/*  Variaveis globais  */global {	int a;}/*  Procedimento para escrever uma cadeia de letras O */

/* ERRO SEMÂNTICO ESPERADO: VARIÁVEL INTEIRA j NÃO DECLARADA */procedure FuncaoTeste ()local {	int i;}statements {	
	j = 0;
	for i init 1 while i <= 5 new i+1 do {
		write ("O");
	}
	
}
/*  Modulo principal  */mainstatements {	call FuncaoTeste ();}}}}