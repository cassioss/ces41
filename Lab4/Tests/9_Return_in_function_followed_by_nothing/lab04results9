program AnaliseDeTexto {{{

global {
	int a;
}

int function FuncaoTeste()
statements {
	return;


***** Incompatibilidade: Funcao deveria retornar um valor *****

}

int function FuncaoTeste3()
statements {
	return 1;
}

main
statements {
}

}}}


  =========================
  = TABELA  DE  SIMBOLOS: =
  =========================

Classe 5:
  (a, IDVAR, INTEIRO, 0, 0, ##global)
  (##global, IDGLOB, NAOVAR, 0, 0)

Classe 8:
  (##main, IDPROG, NAOVAR, 0, 0, ##global)

Classe 17:
  (FuncaoTeste, IDFUNC, INTEIRO, 0, 0, ##global)

Classe 22:
  (FuncaoTeste3, IDFUNC, INTEIRO, 0, 0, ##global)
