program AnaliseDeTexto {{{

global {
	int a;
}

procedure a()
local {
	int i;
}

statements {
	for i init 1 while i <= 5 new i + 1 do {
		write ("O");
	}
}

main
statements {
	call a();
}

}}}


  =========================
  = TABELA  DE  SIMBOLOS: =
  =========================

Classe 5:
  (a, IDPROC, NAOVAR, 0, 0, ##global)
  (a, IDVAR, INTEIRO, 0, 0, ##global)
  (##global, IDGLOB, NAOVAR, 0, 0)

Classe 8:
  (##main, IDPROG, NAOVAR, 0, 0, ##global)

Classe 13:
  (i, IDVAR, INTEIRO, 0, 1, a)
