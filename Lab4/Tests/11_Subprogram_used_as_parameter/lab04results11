program AnaliseDeTexto {{{

global {
	int a;
}

logic function EhBooleano()
statements {
	return true;
}

int function FuncaoZero(logic EhBooleano)
statements {
	return 0;
}

main
statements {
}

}}}


  =========================
  = TABELA  DE  SIMBOLOS: =
  =========================

Classe 5:
  (a, IDVAR, INTEIRO, 0, 0, ##global)
  (##global, IDGLOB, NAOVAR, 0, 0)

Classe 8:
  (##main, IDPROG, NAOVAR, 0, 0, ##global)
  (FuncaoZero, IDFUNC, INTEIRO, 0, 0, ##global)

Classe 22:
  (EhBooleano, IDVAR, LOGICO, 1, 1, FuncaoZero)
  (EhBooleano, IDFUNC, LOGICO, 0, 0, ##global)
