program AnaliseDeTexto {{{global {    logic a;}/* ERRO SEMÂNTICO ESPERADO: FOR RECEBE VARIÁVEL DE TIPO INCORRETO EM INIT, WHILE OU NEW */procedure FuncaoTeste ()local {    int i;}statements {    for i init false while i <= 5 new i+1 do {        write ("O");    }    for i init 0 while 1 new i+1 do {        write ("O");    }    for i init 0 while i <= 5 new false do {        write ("O");    }}/*  Modulo principal  */mainstatements {call FuncaoTeste ();}}}}