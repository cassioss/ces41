program AnaliseDeTexto {{{

global {
	logic a;
}

procedure FuncaoTeste()
local {
	int i;
}

statements {
	for i init false while i <= 5 new i + 1 do {
		write ("O");
	}
	for i init 0 while 1 new i + 1 do {
		write ("O");
	}
	for i init 0 while i <= 5 new false do {
		write ("O");
	}
}

main
statements {
	call FuncaoTeste();
}

}}}


  =========================
  = TABELA  DE  SIMBOLOS: =
  =========================

Classe 5:
  (a, IDVAR, LOGICO, 0, 0, ##global)
  (##global, IDGLOB, NAOVAR, 0, 0)

Classe 8:
  (##main, IDPROG, NAOVAR, 0, 0, ##global)

Classe 13:
  (i, IDVAR, INTEIRO, 0, 1, FuncaoTeste)

Classe 17:
  (FuncaoTeste, IDPROC, NAOVAR, 0, 0, ##global)
