program AnaliseDeTexto {{{

global {
	int a;
}

int function FuncaoTeste(int n)
statements {
	if n < 0 then return 0;
	if n == 0 then return 1;
	return n * FuncaoTeste(n - 1);
}

main
statements {
	call FuncaoTeste();
}

}}}


  =========================
  = TABELA  DE  SIMBOLOS: =
  =========================

Classe 5:
  (a, IDVAR, INTEIRO, 0, 0, ##global)
  (##global, IDGLOB, NAOVAR, 0, 0)

Classe 8:
  (##main, IDPROG, NAOVAR, 0, 0, ##global)

Classe 17:
  (FuncaoTeste, IDFUNC, INTEIRO, 0, 0, ##global)

Classe 18:
  (n, IDVAR, INTEIRO, 1, 1, FuncaoTeste)
